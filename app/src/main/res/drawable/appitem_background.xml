<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >
            <solid android:color="#2B2726"  />
            <corners android:radius="30dp" />

            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="0dp"
                android:top="0dp" />
            <gradient
                android:centerColor="#66000000"
                android:centerX="0.6"
                android:centerY="0.5"
                android:endColor="#1A151313"
                android:gradientRadius="90%"
                android:type="radial"/>
        </shape>
    </item>

    <item >
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    <solid android:color="#59321A"  />
    <corners android:radius="30dp" />
    <stroke
        android:width="5dp"
        android:color="#1A2502" />
</shape>
</item>




</layer-list>